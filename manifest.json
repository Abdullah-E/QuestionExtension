{
    "manifest_version": 3,
    "name": "Question Extension",
    "version": "1.0",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
      }
    ],
    "action": {
      "default_popup": "index.html"
      
    }
  }
  